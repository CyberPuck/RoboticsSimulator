<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<Pane prefHeight="720.0" prefWidth="440.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="UI.tabControls.PointTab">
    <children>
        <Label layoutX="14.0" layoutY="18.0" text="End Point:"/>
        <Label layoutX="14.0" layoutY="43.0" text="X point(ft):"/>
        <TextField fx:id="xEndPoint" layoutX="100.0" layoutY="40.0" prefWidth="100.0"/>
        <Label layoutX="223.0" layoutY="43.0" text="Y point(ft):"/>
        <TextField fx:id="yEndPoint" layoutX="300.0" layoutY="40.0" prefWidth="100.0"/>
        <Label layoutX="14.0" layoutY="75.0" text="End orientation(deg):"/>
        <TextField fx:id="endOrientation" layoutX="180.0" layoutY="75.0"/>
        <Label layoutX="14.0" layoutY="110.0" text="Time to finish(sec):"/>
        <TextField fx:id="timeToFinish" layoutX="180.0" layoutY="110.0"/>
        <Label layoutX="14.0" layoutY="360.0" text="Way Points:      X(ft):                    Y(ft):"/>
        <Label layoutX="14.0" layoutY="395.0" text="Point One:"/>
        <TextField fx:id="oneX" layoutX="101.0" layoutY="395.0" prefHeight="25.0" prefWidth="65.0"/>
        <TextField fx:id="oneY" layoutX="201.0" layoutY="395.0" prefHeight="25.0" prefWidth="65.0"/>
        <Label layoutX="14.0" layoutY="430.0" text="Point Two:"/>
        <TextField fx:id="twoX" layoutX="101.0" layoutY="430.0" prefHeight="25.0" prefWidth="65.0"/>
        <TextField fx:id="twoY" layoutX="201.0" layoutY="430.0" prefHeight="25.0" prefWidth="65.0"/>
        <Label layoutX="14.0" layoutY="465.0" text="Point Three:"/>
        <TextField fx:id="threeX" layoutX="101.0" layoutY="465.0" prefHeight="25.0" prefWidth="65.0"/>
        <TextField fx:id="threeY" layoutX="201.0" layoutY="465.0" prefHeight="25.0" prefWidth="65.0"/>
        <Button fx:id="startButton" layoutX="170.0" layoutY="530.0" mnemonicParsing="false" prefHeight="50.0"
                prefWidth="100.0" text="Start">
            <font>
                <Font size="24.0"/>
            </font>
        </Button>
        <Label layoutX="14.0" layoutY="600.0" text="All angles are based of the positive y-axis 0 degrees"/>
        <Label layoutX="14.0" layoutY="625.0" text="Positive rotation is in the counter-clockwise direction"/>
    </children>
</Pane>
